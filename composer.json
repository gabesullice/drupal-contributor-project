{
    "name": "drupal/contributor-project",
    "type": "project",
    "description": "A project template for doing Drupal core and contrib work.",
    "license": "proprietary",
    "require": {
        "cweagans/composer-patches": "^1.6",
        "drupal/contrib-installer": "dev-master",
        "drupal/drupal": "*@dev",
        "drupal/core-dev": "*@dev"
    },
    "config": {
        "preferred-install": {
            "drupal/*": "source"
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-disable": [
            "drupal"
        ]
    },
    "repositories": {
        "drupal-contrib-installer-plugin": {
            "type": "vcs",
            "url": "https://github.com/gabesullice/drupal-contrib-installer.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    }
}
